{
  "name": "zanysoft/laravel-meta-tags",
  "keywords": [
    "meta tags",
    "meta",
    "tags",
    "head",
    "laravel"
  ],
  "description": "A package to manage Header Meta Tags",
  "license": "MIT",
  "authors": [
    {
      "name": "Mubashar",
      "email": "info@zanysoft.net"
    }
  ],
  "require": {
    "php": ">=5.5.9",
    "illuminate/support": "~5.1|^6.0|~7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.0",
    "mockery/mockery": "^0.9.4"
  },
  "autoload": {
    "psr-4": {
      "ZanySoft\\LaravelMetaTags\\": "src/ZanySoft/LaravelMetaTags/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "ZanySoft\\LaravelMetaTags\\MetaTagsServiceProvider"
      ],
      "aliases": {
        "MetaTag": "ZanySoft\\LaravelMetaTags\\Facades\\MetaTag"
      }
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
