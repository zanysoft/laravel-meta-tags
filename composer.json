{
  "name": "zanysoft/laravel-meta-tags",
  "description": "A package to manage Header Meta Tags",
  "license": "MIT",
  "homepage": "http://www.zanysoft.net",
  "keywords": [
    "meta tags",
    "meta",
    "tags",
    "head",
    "laravel"
  ],
  "authors": [
    {
      "name": "Zany Soft",
      "email": "info@zanysoft.net",
      "homepage": "http://www.zanysoft.net"
    }
  ],
  "require": {
    "php": "^7.1 || ^8",
    "illuminate/support": "~5.1|^6.0|~7.0|~8.0"
  },
  "autoload": {
    "psr-4": {
      "ZanySoft\\LaravelMetaTags\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "ZanySoft\\LaravelMetaTags\\MetaTagsServiceProvider"
      ],
      "aliases": {
        "MetaTag": "ZanySoft\\LaravelMetaTags\\Facades\\MetaTag"
      }
    }
  },
  "minimum-stability": "stable"
}
